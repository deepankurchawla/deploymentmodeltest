    <validationRules>
        <fullName>Meta_Data_Record_Count</fullName>
        <active>false</active>
        <errorConditionFormula>Meta_Data_Record_Count__c &gt;1</errorConditionFormula>
        <errorMessage>Against one Scrum Task, only one Meta Data Record can be inserted. Please edit the previous record to amend the details or else get in touch with your System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Members_record_count</fullName>
        <active>true</active>
        <errorConditionFormula>Team_Member_Record_Count__c  &gt;1 &amp;&amp;  Request_Additional_Resources__c  = False</errorConditionFormula>
        <errorMessage>Against one Scrum Task, only one Team Members Record can be inserted at a time. Please edit the previous Team Member record to amend the details or else get in touch with Scrum Owner to Request for Additional Resources.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>When_Key_Resource_is_Null</fullName>
        <active>true</active>
        <errorConditionFormula>(Team_Member_Record_Count__c  &lt;1) &amp;&amp;  value( substitute(text(Scrum_Task_Progress__c ),&apos;%&apos;,&apos;&apos;))&gt;30</errorConditionFormula>
        <errorMessage>Please assign Team Members at the beginning of Solution Design stage to proceed. Key Team Members can be assigned in &apos;Team Members&apos; module for this Scrum Requirement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_not_greater_than_End_Date</fullName>
        <active>true</active>
        <description>Start Date not greater than End Date</description>
        <errorConditionFormula>Scrum_End_Date__c &lt;  Scrum_Start_Date__c</errorConditionFormula>
        <errorMessage>Scrum End Date cannot be less than Scrum Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meta_Data_less_than_1_an_stage_Unit_Test</fullName>
        <active>true</active>
        <description>Restricts Scrum stage to go beyond Unit Test if Meta Data Record of the parent Scrum is less than 1</description>
        <errorConditionFormula>(Meta_Data_Record_Count__c &lt;1) &amp;&amp;  value( substitute(text(Scrum_Task_Progress__c ),&apos;%&apos;,&apos;&apos;))&gt;50</errorConditionFormula>
        <errorMessage>Relevant Meta Data Details have to be entered by you for the Configuration/Customization done for this Scrum Requirement. Scrum Stage cannot move beyond &apos;Unit Test&apos; until details validated.</errorMessage>
    </validationRules>
